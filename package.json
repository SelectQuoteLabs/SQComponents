{
  "name": "scplus-shared-components",
  "version": "1.1.9",
  "description": "SC Plus shared component library",
  "main": "dist/bundle.cjs.min.js",
  "module": "dist/bundle.esm.min.js",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "lint": "eslint 'src/**/*.js'",
    "lint.fix": "eslint 'src/**/*.js' --fix",
    "lint.staged": "lint-staged",
    "semver": "npm version",
    "deploy": "func() { npm run semver \"$1\" && npm pack && node s3.js; }; func"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/SelectQuote/scplus-shared-components.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/SelectQuote/scplus-shared-components#readme",
  "dependencies": {
    "@material-ui/core": "^4.4.3",
    "aws-sdk": "^2.552.0",
    "chalk": "^2.4.2",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "material-ui": "^0.20.2",
    "prop-types": "^15.7.2",
    "react": "^16.10.1",
    "react-dom": "^16.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.22.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2"
  }
}
