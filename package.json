    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react-query": "^2.26.2",
    "react-text-mask": "^5.4.3",
    "react-window": "^1.8.5",
    "yup": "^0.28.3"
  },
  "peerDependencies": {
    "@tippy.js/react": "^3.1.1",
    "ag-grid-community": "^22.1.1",
    "ag-grid-react": "^22.1.1",
    "react": "^16.12.0",
    "react-datepicker": "^2.13.0",
    "react-dom": "^16.12.0",
    "react-weekly-schedule": "0.0.0-development"
  },
