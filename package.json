{
  "name": "scplus-shared-components",
  "version": "7.1.0",
  "description": "SC Plus shared component library",
  "main": "dist/index.js",
  "source": [
    "dist/index.js"
  ],
  "private": true,
  "sideEffects": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "parcel build src/index.js",
    "lint": "eslint 'src/**/*.js'",
    "lint.fix": "eslint 'src/**/*.js' --fix",
    "lint.staged": "lint-staged",
    "publish": "node s3.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "nuke-everything": "rm -rf ./dist && rm -rf ./node_modules && rm -f package-lock.json",
    "commit": "npx git-cz"
  },
  "files": [
    "/dist"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "http://bitbucket.org/SelectQuote/scplus-shared-components"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/SelectQuote/scplus-shared-components#readme",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.46",
