{
  "name": "scplus-shared-components",
  "version": "1.1.17",
  "description": "SC Plus shared component library",
  "main": "dist",
  "module": "dist",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "parcel build src/index.js",
    "lint": "eslint 'src/**/*.js'",
    "lint.fix": "eslint 'src/**/*.js' --fix",
    "lint.staged": "lint-staged",
    "semver": "npm version",
    "deploy": "npm run build && func() { npm run semver \"$1\" && npm pack && node s3.js; }; func",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "files": [
    "/dist",
    "/src"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/SelectQuote/scplus-shared-components.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/SelectQuote/scplus-shared-components#readme",
  "dependencies": {
    "@material-ui/core": "^4.4.3",
    "aws-sdk": "^2.552.0",
    "chalk": "^2.4.2",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "material-ui": "^0.20.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@material-ui/icons": "^4.5.1",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-docs": "^5.3.9",
    "@storybook/addon-info": "^5.3.9",
    "@storybook/addon-knobs": "^5.3.9",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addon-notes": "^5.3.9",
    "@storybook/addons": "^5.2.6",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.2.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-docgen": "^4.1.0",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "storybook-addon-material-ui": "^0.9.0-alpha.20"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
